@model StoreMVC.Models.Customer
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>@Model.Name</h1>
<ul>
    <li>Membership Type: @Model.MemberShipType.Name</li>
    @{ 
        if (Model.Birthdate != null)
        {
            <li>Birthdate: @Model.Birthdate</li>
        }
    }
</ul>
    

